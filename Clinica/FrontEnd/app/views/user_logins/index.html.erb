<p id="notice"><%= notice %></p>

<h1>User Logins</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Last name</th>
      <th>User name</th>
      <th>Email</th>
      <th>Password digest</th>
      <th>Birth date</th>
      <th>Estatus</th>
      <th>Avatar url</th>
      <th>Phone</th>
      <th>Identification</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_logins.each do |user_login| %>
      <tr>
        <td><%= user_login.name %></td>
        <td><%= user_login.last_name %></td>
        <td><%= user_login.user_name %></td>
        <td><%= user_login.email %></td>
        <td><%= user_login.password_digest %></td>
        <td><%= user_login.birth_date %></td>
        <td><%= user_login.estatus %></td>
        <td><%= user_login.avatar_url %></td>
        <td><%= user_login.phone %></td>
        <td><%= user_login.identification %></td>
        <td><%= link_to 'Show', user_login %></td>
        <td><%= link_to 'Edit', edit_user_login_path(user_login) %></td>
        <td><%= link_to 'Destroy', user_login, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Login', new_user_login_path %>
