<p id="notice"><%= notice %></p>

<h1>Type Identifications</h1>

<table>
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_identifications.each do |type_identification| %>
      <tr>
        <td><%= type_identification.codigo %></td>
        <td><%= type_identification.description %></td>
        <td><%= link_to 'Show', type_identification %></td>
        <td><%= link_to 'Edit', edit_type_identification_path(type_identification) %></td>
        <td><%= link_to 'Destroy', type_identification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type Identification', new_type_identification_path %>
